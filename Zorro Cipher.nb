Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"AES", " ", "Key", " ", "Schedule", " ", "Implementation"}], 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"AESKeyExpansion", "[", "key_", "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Nk", "=", "4"}], ",", 
         RowBox[{"(*", 
          RowBox[{
           RowBox[{"Key", " ", "length", " ", "in", " ", "32"}], "-", 
           RowBox[{"bit", " ", "words"}]}], "*)"}], "\[IndentingNewLine]", 
         RowBox[{"Nr", "=", "10"}], ",", 
         RowBox[{"(*", 
          RowBox[{"Number", " ", "of", " ", "rounds"}], "*)"}], 
         "\[IndentingNewLine]", "w", ",", "i", ",", "temp", ",", 
         "\[IndentingNewLine]", 
         RowBox[{"Rcon", "=", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"1", ",", "0", ",", "0", ",", "0"}], "}"}], ",", 
